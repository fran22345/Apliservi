{
  "cli": {
    "version": ">= 12.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "DATABASE_URL": "@DATABASE_URL",
        "URLWEB_CLIENT_ID": "@URLWEB_CLIENT_ID"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "DATABASE_URL": "@DATABASE_URL",
        "URLWEB_CLIENT_ID": "@URLWEB_CLIENT_ID"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
